package bai5;

public class SinhVien {

	private String hoten;
	private String id;
	private String lop;
	
	public String getHoten() {
		return hoten;
	}
	public void setHoten(String hoten) {
		this.hoten = hoten;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public SinhVien(String hoten, String id) {
		super();
		this.hoten = hoten;
		this.id = id;
	}
	public String getLop() {
		return lop;
	}
	public void setLop(String lop) {
		this.lop = lop;
	}
	public SinhVien(String hoten, String id, String lop) {
		super();
		this.hoten = hoten;
		this.id = id;
		this.lop = lop;
	}
	@Override
	public String toString() {
		return "SinhVien [hoten=" + hoten + ", id=" + id + ", lop=" + lop + "]";
	}

	
	
}
