package bai5;

import java.util.Date;

public class TheMuon extends SinhVien{

	private  String	sophieumuon ;
	private  Date ngaymuon;
	private  Date hantra;
	private  String sohieusach;
	
	public String getSophieumuon() {
		return sophieumuon;
	}
	public void setSophieumuon(String sophieumuon) {
		this.sophieumuon = sophieumuon;
	}
	public Date getNgaymuon() {
		return ngaymuon;
	}
	public void setNgaymuon(Date ngaymuon) {
		this.ngaymuon = ngaymuon;
	}
	public Date getHantra() {
		return hantra;
	}
	public void setHantra(Date hantra) {
		this.hantra = hantra;
	}
	public String getSohieusach() {
		return sohieusach;
	}
	public void setSohieusach(String sohieusach) {
		this.sohieusach = sohieusach;
	}
	public TheMuon(String hoten, String id, String sophieumuon, Date ngaymuon, Date hantra, String sohieusach) {
		super(hoten, id);
		this.sophieumuon = sophieumuon;
		this.ngaymuon = ngaymuon;
		this.hantra = hantra;
		this.sohieusach = sohieusach;
	}
	
	


}
